<h3>
    Employees with Last Name: {{{lastname}}}
</h3>
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
           {{#each data}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.firstName}}</td>
                <td>{{this.lastName}}</td>
            </tr>
            {{/each}}
    </tbody>
</table>