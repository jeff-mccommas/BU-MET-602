<h3>
    All Employees
</h3>
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
           {{#each data}}
            <tr>
                <td><a href="/employee/{{this.id}}">{{this.id}}</a></td>
                <td>{{this.firstName}}</td>
                <td>{{this.lastName}}</td>
            </tr>
            {{/each}}
    </tbody>
</table>