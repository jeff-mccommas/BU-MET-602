<h3>
    Employees with Id : {{{id}}}
</h3>
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
            <tr>
                <td> 
                {{data.id}}
                </td>
                <td>{{data.firstName}}</td>
                <td>{{data.lastName}}</td>
            </tr>
    </tbody>
</table>
<form method="GET" action="/employeedelete/{{id}}">
    <input type="submit" value="Delete"/>
</form>
<a href="/editemployee/{{id}}">
<input type="button" value="Edit"/></a>